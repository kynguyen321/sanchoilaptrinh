        hls.attachMedia(video);
        hls.on(Hls.Events.MANIFEST_PARSED,function()
        {
            video.play();
        });
    }
    else if (video.canPlayType('application/vnd.apple.mpegurl'))
    {
        video.src = 'https://stream.unica.vn/unica/_definst_/unica/221/22013/720.mp4/playlist.m3u8?wowzatokenendTime=1655830705&wowzatokenstartTime=1655823505&wowzatokenhash=ld6YYXU3ugzto69CswM1_nfezok2bjwHsIhuT4PdLZM=&uid=546742&expired=1655824617&token=74187b0015185374711c2370d78ad4c1117525aa';
